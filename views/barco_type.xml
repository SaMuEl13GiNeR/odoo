<odoo>
    <data>
        <!-- explicit list view definition -->


        <!-- BARCO TYPE barco_type.xml-->

        <record model="ir.ui.view" id="pirates.barco_type_tree">
            <field name="name">barco type list</field>
            <field name="model">pirates.barco_type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"></field>
                    <field name="avatar_min" widget="image"></field>
                    <field name="dmg"></field>
                    <field name="hp"></field>
                    <field name="ataques"></field>
                </tree>
            </field>
        </record>
    </data>


    <record model="ir.ui.view" id="pirates.barco_type_form">
        <field name="name">barco type form</field>
        <field name="model">pirates.barco_type</field>
        <field name="arch" type="xml">
            <form string="Barco Type Form">
                <sheet>
                    <group>
                        <field name="name"></field>
                        <field name="dmg"></field>
                        <field name="hp"></field>
                        <field name="ataques"></field>
                        <field name="avatar" widget="image" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>


</odoo>